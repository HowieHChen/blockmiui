<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="false"
        android:color="@color/hyperx_list_secondary_text_color_disable" />
    <item
        android:state_selected="true"
        android:color="@color/hyperx_list_secondary_text_color_pressed" />
    <item
        android:color="@color/hyperx_list_secondary_text_color_checked"
        android:state_activated="true" />
    <item
        android:state_checked="true"
        android:color="@color/hyperx_list_secondary_text_color_checked" />
    <item
        android:state_pressed="true"
        android:color="@color/hyperx_list_secondary_text_color_pressed" />
    <item android:color="@color/hyperx_list_secondary_text_color_normal" />
</selector>
